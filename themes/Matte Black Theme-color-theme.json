	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#ffffff30"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"constant.other.symbol"
			],
			"settings": {
				"foreground": "#95a967"
			}
		},
		{
			"name": "Regular Expressions and Escape Characters",
			"scope": [
				"string.regexp",
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#96beff"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#96beff"
			}
		},
		{
			"name": "Built-in constants",
			"scope": [
				"constant.language"
			],
			"settings": {
				"foreground": "#96beff"
			}
